#########################################################
fields:f75de5f0-ac1f-479f-9c05-5c056eb9f5fc a form:Field ;
    mu:uuid "f75de5f0-ac1f-479f-9c05-5c056eb9f5fc" ;
    sh:name "Bestanden" ;
    sh:order 10001 ;
    sh:path dct:hasPart;
    form:validations
     [ a  form:RequiredConstraint ;
          form:grouping form:Bag ;
          sh:path dct:hasPart;
          sh:resultMessage "Gelieve minstens één URL of bestand op te geven."@nl # TODO: later custom validator
     ];
    form:displayType displayTypes:files; # consider this v1.0
    form:help """Voeg hier de brief toe waarin je het eredienstbestuur en het centraal bestuur op de hoogte brengt van de onvolledigheid en onontvankelijkheid van een specifieke budget(wijziging) of meerjarenplan(wijziging). Door het versturen van deze brief wordt het eredienstbestuur geïnformeerd dat de toezichttermijn eigenlijk nog niet loopt.""" ;
    sh:group fields:aDynamicPropertyGroup .

